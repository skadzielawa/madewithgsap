{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "madewithgsap/effect-000",
	"version": "0.1.0",
	"title": "Madewithgsap - Effect 000",
	"category": "widgets",
	"description": "A simple block, listing recent posts and using Effect 000 from the madewithgsap.com website.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"maxPosts": {
			"type": "integer",
			"default": 12
		},
		"order": {
			"type": "string",
			"default": "desc"
		},
		"orderBy": {
			"type": "string",
			"default": "date"
		}
	},
	"textdomain": "madewithgsap",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
